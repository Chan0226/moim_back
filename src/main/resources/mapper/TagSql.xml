<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mapper.TagMapper">

    <select id="selectTagList" parameterType="int" resultType="TagDto">
        select tname from tag where roomNum=#{num}
    </select>


    <insert id="insertTag" parameterType="java.util.HashMap">
        insert into tag (tname, roomNum)
        values
        <foreach collection="Taglist" item="item" separator=",">
            (#{item.tname}, #{item.roomNum})
        </foreach>
    </insert>

</mapper>
