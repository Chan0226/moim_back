<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.mapper.CategoryMapper">

    <!--  메인페이지 카테고리 목록 출력  -->
    <select id="getAllCategory" resultType="CategoryDto">
        select * from category
    </select>

    <insert id="insertCategory" parameterType="java.util.HashMap">
        insert into roomCategory (roomNum, categoryNum)
        values
        <foreach collection="CateList" item="item" separator=",">
            (#{item.roomNum}, #{item.categoryNum})
        </foreach>
    </insert>


</mapper>